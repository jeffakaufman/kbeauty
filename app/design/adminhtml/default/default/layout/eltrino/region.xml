<layout>
    <default>
        <reference name="head">
            <action method="addCss"><name>eltrino/region.css</name></action>
        </reference>
    </default>
    <adminhtml_customer_edit>
        <reference name="content">
            <block type="adminhtml/template" name="eltrino_region_js" template="eltrino/customer/edit/js.phtml"></block>
        </reference>
    </adminhtml_customer_edit>
    <adminhtml_region_index>
        <reference name="content">
            <block type="eltrino_region/adminhtml_region" name="eltrino_region_index" />
        </reference>
    </adminhtml_region_index>
    <adminhtml_region_edit>
        <reference name="content">
            <block type="eltrino_region/adminhtml_region_edit" name="eltrino_region_edit" />
        </reference>
    </adminhtml_region_edit>
</layout>
